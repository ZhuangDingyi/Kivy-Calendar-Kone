<Select>:
	lbl_: lbl
	Image:
		source:'logo.jpg'
		keep_ratio: False
        allow_stretch: True
        opacity: 0.8
        size_hint: 0.3, 1
        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
		
	Label:
		id: lbl
		text_size: self.size
    	halign: 'right'
    	valign: 'middle'
		font_size:'32sp'
		color: 0,0,0,1
		
